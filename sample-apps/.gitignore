# Shared .gitignore file generated by Rails for the sample-apps.

# Ignore bundler config.
/*-sample/.bundle

# Ignore the default SQLite database.
/*-sample/db/*.sqlite3
/*-sample/db/*.sqlite3-*

# Ignore all logfiles and tempfiles.
/*-sample/log/*
/*-sample/tmp/*
!/*-sample/log/.keep
!/*-sample/tmp/.keep

# Ignore pidfiles, but keep the directory.
/*-sample/tmp/pids/*
!/*-sample/tmp/pids/
!/*-sample/tmp/pids/.keep

/*-sample/public/assets

# Ignore master key for decrypting credentials and more.
/*-sample/config/master.key
